set_perm(0, 0, 0777, "/tmp/installbusybox");
run_program("/tmp/installbusybox");
assert(package_extract_file("boot.img", "/tmp/boot.img"),
       write_raw_image("/tmp/boot.img", "boot"),
       delete("/tmp/boot.img"));
show_progress(10.000000, 1);
unmount("/system");
unmount("/data");