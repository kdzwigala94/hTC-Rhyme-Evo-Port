assert(getprop("ro.product.device") == "supersonic" || getprop("ro.build.product") == "supersonic" || getprop("ro.product.board") == "supersonic");
show_progress(0, 0);
mount("MTD", "system", "/system");
mount("MTD", "userdata", "/data");
show_progress(0.700000, 50);
